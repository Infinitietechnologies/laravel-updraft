{
    "name": "infinitietechnologies/updraft",
    "description": "A Laravel package for easily updating your application through update packages",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Infinitie Technologies",
            "email": "contact@infinitietechnologies.com",
            "homepage": "https://github.com/Infinitietechnologies"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelUpdraft\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelUpdraft\\UpdraftServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}